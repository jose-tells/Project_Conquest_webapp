.gridPhotos
    width 90%
    margin 1.3rem auto 0 auto
    display grid 
    grid-template-columns repeat(5, 1fr)
    grid-template-rows repeat(21, .3fr)
    gap .2rem .2rem

.photo1
    grid-column 1/4
    grid-row 1/3
    overflow hidden
    transition(.3s)
    img
        width 102%

